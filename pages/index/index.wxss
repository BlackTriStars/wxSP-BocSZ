}
.index .section .main .mainItem .item {
  width: 340.27777778rpx;
  border: 0.69444444rpx solid #dcdcdc;
  /*no*/
}
.index .section .main .mainItem .item image {
  width: 100%;
  display: block;
}
.index .section .main .mainItem .item p {
  font-size: 25rpx;
  color: #333;
